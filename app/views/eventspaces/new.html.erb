<div class="container">
  <!-- Content here -->
    <h1>Add a new event space</h1>
    <%= simple_form_for @eventspace do |f| %>
        <%= f.input :name %>
        <%= f.input :address %>
        <%= f.input :price %>
        <%= f.input :description %>
        <%= f.input :photos, as: :file, input_html: { multiple: true }%>
        <%= f.submit "Create my eventspace", :class => "button" %>
    <% end %>
</div>

