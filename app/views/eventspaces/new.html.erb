
  <h3>Add a new event space</h3>
    <%= simple_form_for @eventspace do |f| %>
        <%= f.input :name %>
        <%= f.input :address %>
        <%= f.input :price %>
        <%= f.input :description %>
        <%= f.input :photos, as: :file, input_html: { multiple: true }%>
        <%= f.submit %>
    <% end %>
